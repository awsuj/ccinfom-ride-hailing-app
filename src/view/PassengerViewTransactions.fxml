<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<BorderPane prefHeight="300.0" prefWidth="463.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.src.controllers.PassengerController">
    <top>
        <HBox alignment="TOP_CENTER" prefHeight="22.0" prefWidth="389.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Label fx:id="passengerNameLabel" text="passengerNameLabel" />
                <Pane prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                <Label fx:id="balanceLabel" text="balanceLabel" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
        </HBox>
    </top>
    <center>
        <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
            <children>
                <Label text="Your Transactions">
                    <padding><Insets bottom="20.0" /></padding>
                    <font><Font size="18.0" /></font>
                </Label>
                <TableView fx:id="transactionsTable" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="colTime" prefWidth="120.0" text="Date/Time" />
                        <TableColumn fx:id="colPickup" prefWidth="100.0" text="Pickup" />
                        <TableColumn fx:id="colDropoff" prefWidth="100.0" text="Dropoff" />
                        <TableColumn fx:id="colPrice" prefWidth="67.0" text="Fare" />
                        <TableColumn fx:id="colStatus" prefWidth="77.0" text="Status" />
                    </columns>
                </TableView>
                <Button fx:id="backButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onBackToMenuClicked" text="Back" />
            </children>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="30.0" />
            </padding>
        </VBox>
    </center>
</BorderPane>